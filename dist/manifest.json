{
  "manifest_version": 2,

  "name": "HLCOK",
  "description": "This chrome extension is hide Like counts on Kintone",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/like-counts-is-hidden.png"
  },

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/content_script.js"]
      }
  ],

  "background": {
    "scripts": ["js/background.js"]
  },

  "permissions": [
    "<all_urls>"
  ]
}